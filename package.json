{
  "name": "overlay",
  "productName": "Overlay",
  "version": "1.0.0",
  "description": "AI-powered dictation app that transcribes speech and inserts text into any application",
  "main": ".webpack/main",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron-forge start",
    "package": "cross-env NODE_ENV=production electron-forge package",
    "make": "cross-env NODE_ENV=production electron-forge make",
    "publish": "cross-env NODE_ENV=production electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [
    "dictation",
    "speech-to-text",
    "AI",
    "productivity"
  ],
  "author": {
    "name": "Abhishekucs",
    "email": "abhishekucskumar@gmail.com"
  },
  "build": {
    "mac": {
      "hardenedRuntime": true,
      "entitlements": "entitlements.mac.plist",
      "extendInfo": {
        "NSMicrophoneUsageDescription": "This app requires microphone access to capture audio for dictation.",
        "NSSystemAdministrationUsageDescription": "This app requires system access to insert text into other applications."
      }
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.8.3",
    "@electron-forge/maker-rpm": "^7.8.3",
    "@electron-forge/maker-squirrel": "^7.8.3",
    "@electron-forge/maker-zip": "^7.8.3",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.3",
    "@electron-forge/plugin-fuses": "^7.8.3",
    "@electron-forge/plugin-webpack": "^7.8.3",
    "@electron/fuses": "^1.8.0",
    "@types/ffprobe-static": "^2.0.3",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "css-loader": "^6.11.0",
    "electron": "37.2.6",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.32.0",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "node-loader": "^2.1.0",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.17",
    "ts-loader": "^9.5.2",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.56.0",
    "@types/fluent-ffmpeg": "^2.1.27",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.6.2",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "lucide-react": "^0.541.0",
    "node-clipboard": "^1.1.2",
    "openai": "^4.104.0",
    "posthog-js": "^1.260.2",
    "posthog-node": "^5.7.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "robotjs": "^0.6.0",
    "tailwind-merge": "^3.3.1"
  }
}
